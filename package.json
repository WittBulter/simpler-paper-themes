{
  "name": "simpler-paper-themes",
  "version": "0.3.1",
  "description": "this rep generates themes for Simpler Paper",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./build/webpack.dev.js --progress --watch --port 4001",
    "build": "rm -rf ./dist && webpack --config ./build/webpack.prod.js --progress",
    "commit": "node_modules/commitizen/bin/commitizen init cz-conventional-changelog --save --save-exact",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DhyanaChina/simpler-paper-themes.git"
  },
  "files": [
    "LICENSE",
    "dist/"
  ],
  "author": "DhyanaChina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DhyanaChina/simpler-paper-themes/issues"
  },
  "homepage": "https://github.com/DhyanaChina/simpler-paper-themes#readme",
  "devDependencies": {
    "@commitlint/config-angular": "^5.1.1",
    "commitizen": "^2.9.6",
    "commitlint": "^5.2.5",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "node-sass": "^4.6.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
